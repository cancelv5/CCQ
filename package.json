{
  "name": "ccq-cli",
  "version": "0.1.2",
  "description": "koa project with typescript generator",
  "main": "./bin/index.js",
  "bin": {
    "ccq": "./bin/ccq.js"
  },
  "scripts": {
    "dev": "nodemon --watch 'app/**/*' --watch 'lib/**/*' --delay 2 --signal SIGHUP -e ts,tsx --exec 'ts-node' ./app/server.ts",
    "build": "yarn clean && tsc",
    "start": "cd ./dist && node ./app/server.js",
    "clean": "rm -rf dist"
  },
  "author": "cancel wu",
  "license": "ISC",
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/koa": "^2.0.46",
    "@types/koa-router": "^7.0.32",
    "@types/lodash": "^4.14.116",
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "commander": "^2.19.0",
    "glob": "^7.1.3",
    "inquirer": "^6.2.1",
    "koa": "^2.5.3",
    "koa-better-body": "^3.0.4",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.11",
    "metalsmith": "^2.3.0",
    "reflect-metadata": "^0.1.12",
    "shelljs": "^0.8.3",
    "sqlite3": "^4.0.4",
    "typeorm": "^0.2.8"
  }
}
